numero1 = int(input("Qual o primeiro número desejado?"))
numero2 = int(input("Qual o segundo número?"))

if numero1 < numero2:
    while numero1 <= numero2:
        print (numero1)
        numero1 = numero1 + 1
else: 
    numero1 > numero2
    while numero1 >= numero2:
        print (numero1)
        numero1 = numero1 - 1
